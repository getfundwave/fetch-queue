{
  "name": "@fundwave/fetchq",
  "version": "0.0.0-init.0",
  "description": "Queue for fetch requests",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "npx eslint src/index.ts",
    "docs": "npx jsdoc-to-markdown dist/index.js > Documentation.md"
  },
  "jest": {
    "clearMocks": true,
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getfundwave/fetch-queue.git"
  },
  "keywords": [
    "js",
    "fetch",
    "queue"
  ],
  "author": "The Fundwave Authors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/getfundwave/fetch-queue/issues"
  },
  "homepage": "https://github.com/getfundwave/fetch-queue#readme",
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "ts-jest": "^29.1.1"
  }
}
